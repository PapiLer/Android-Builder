[
  {
    "kernelSource": {
      "name": "Redmi7（GK）",
      "repo": "https://github.com/PapiLer/Project-KARA",
      "branch": "tmp-ksu",
      "device": "onclite",
      "defconfig": "onclite-perf_defconfig"
    },
    "enableCcache": true,
    "toolchains": [
      {
        "repo": "https://github.com/PapiLer/clang-gengkapak",
        "branch": "13",
        "name": "clang-gengkapak",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CC": "clang-gengkapak/bin/clang",
      "externalCommands": {
        "CROSS_COMPILE": "clang-gengkapak/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "clang-gengkapak/bin/arm-linux-gnueabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/PapiLer/AnyKernel3",
        "branch": "master"
      }
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
